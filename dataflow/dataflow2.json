{
	"name": "dataflow2",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "errorHandling_DS",
						"type": "DatasetReference"
					},
					"name": "sourceData"
				}
			],
			"sinks": [],
			"transformations": [
				{
					"name": "splitCondition"
				}
			],
			"scriptLines": [
				"source(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     ignoreNoFilesFound: false) ~> sourceData",
				"sourceData split(isNull(toDate(SalesDate,'dd-MMM-yyyy')),",
				"     disjoint: false) ~> splitCondition@(BadRows, GoodRows)"
			]
		}
	}
}